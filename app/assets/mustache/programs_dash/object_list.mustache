{{! dashboard object listing}}
<ul class="tree-structure simple single-row-tabs">
{{#list}}
  <li>
    {{#pack this tooltip_view=tooltip_view object_type=object_type object_display=object_display object_route=object_route}}
    <a data-popover-trigger="sticky-hover" data-popover-trigger="sticky-hover" data-sticky-popover="true" data-content="{{render tooltip_view}}">
      {{{withattr "href" "/{object_route}/{id}" "data-original-title" "<h3>{object_display} Info</h3>" }}}
      <div class="row-fluid">
        <div class="span12">
          <div class="item-identifier">
            <span class="key">
              <span>{{prefix}}</span> <span>{{slug}}</span>
            </span>
            <span class="name">
              {{title}}
            </span>
          </div>
        </div>
      </div>
    </a>
    {{/pack}}
  </li>
{{/list}}
</ul>