-content_for :header do
  %ol#breadcrumb
    %li PROGRAMS
    %li=link_to @cycle.program.title, flow_program_path(@cycle.program_id)
    %li.last AUDIT
  .location#pageTitle #{@cycle.program.title} Audit

-content_for :page_help, 'cycle'

-content_for :subheader_extras do
  %h1.fltrt.highlighted#pageType Audit Manager

.row-static
  .doubleColumn
    .doubleWide.halfHeight
      =render :partial => 'cycles/main',
              :layout => 'layouts/widget_box',
              :locals => { :cycle => @cycle }

      -if has_feature?(:BETA)
        .singleColumn
          =render :partial => 'cycles/scope',
                  :layout => 'layouts/widget_box',
                  :locals => { :cycle => @cycle }

      .singleColumn
        =render :partial => 'cycles/workflows',
                :layout => 'layouts/widget_box',
                :locals => { :cycle => @cycle }

  .singleColumn
    -if has_feature?(:BETA)
      =render :partial => 'cycles/pbcimport',
              :layout => 'layouts/widget_box',
              :locals => { :cycle => @cycle }

    =render :partial => 'cycles/pbclist',
            :layout => 'layouts/widget_box',
            :locals => { :cycle => @cycle }

    .fullHeight
      =render :partial => 'cycles/info',
              :layout => 'layouts/widget_box',
              :locals => { :cycle => @cycle }

    -if has_feature?(:BETA)
      .halfHeight
        =render :partial => 'cycles/schedule',
                :layout => 'layouts/widget_box',
                :locals => { :cycle => @cycle }

