-content_for :tooltip_title do
  %h3.oneline
    @control.title Information

.cleanlabels
  .row-fluid
    .span4
      %label Code
      %p=@control.slug
    .span8
      %label Title
      %p=@control.title

  %br/
  .row-fluid
    .span12
      %label Description
      =raw @control.description

  %br/
  .row-fluid
    .span6
      %label Version
      %p ---
    .span6
      %label Deprecated
      %p ---
