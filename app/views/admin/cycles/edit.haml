.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), cycles_path
      %li=link_to pat(:new), new_cycle_path
      %li.active=link_to pat(:edit), edit_cycle_path(@cycle)
  .content
    %h2.title
      =pat(:edit)
      =mt(:cycle)
    .inner
      =form_for @cycle, :method => :put, :html => {:class => :form} do |f|
        =render :partial => "admin/cycles/form", :locals => { :f => f }

/-content_for :sidebar, partial("base/sidebar")
