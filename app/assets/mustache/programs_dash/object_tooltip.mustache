  <div class="row-fluid">
    <div class="span12">
      <h3>
        <i class="grcicon-{{object_type}}-black"></i>
        {{prefix}}{{slug}}
        <span>{{title}}</span>
      </h3>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <h4>Description</h4>
      <div class="rtf">{{{description}}}</div>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span6 created">
      <strong>Created:</strong>
      {{created_date}}
    </div>
    <div class="span6 updated">
      <strong>Updated:</strong>
      {{modified_date}}
    </div>
  </div>