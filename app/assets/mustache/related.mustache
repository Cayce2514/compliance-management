{{#this}}
  <ul class="tree">
    <li class="category">
      {{#with relationship_type}}
      <h3>
        {{ title }}
        <span class="sub-title">{{ description }}</span>
      </h3>
      <div class="item-actions">
        <a class="widgetbtn pull-right" href="{{edit_url}}" data-toggle="modal-ajax-relationship-selector" data-modal-reset="reset" data-tab-target="#related-objects-{{id}}" data-modal-class="modal-wide"><i class="grcicon-link-new"></i></a>
      </div>
      {{/with}}
      <section class="content">
        <ul class="tree" id="related-objects-{{relationship_type.id}}">
      {{#related_objects}}
          <li class="main">
            <a href="/{{ url }}" data-popover-href="/{{ url }}/tooltip" data-popover-trigger="sticky-hover">
              {{#is_beta}}
              <div class="item-identifier">
                <div class="item-title">
                  <div class="row-fluid">
                    <div class="span12">
                      <span class="key">
                        United Kingdom
                      </span>
                      <span class="addition-info">
                        Review
                      </span>
                    </div>
                  </div>
                </div>
              </div>
              {{/is_beta}}
              <div class="item-identifier">
                <div class="item-title">
                  <div class="row-fluid">
                    <div class="span12">
                      <span class="key">
                        {{ object.slug }}
                      </span>
                      <span class="addition-info">
                        {{ object.title }}
                      </span>
                    </div>
                  </div>
                </div>
              </div>
              {{#is_beta}}
              <div class="item-identifier">
                <div class="item-title">
                  <div class="row-fluid">
                    <div class="span4">
                      <strong>239</strong>
                      Objects
                    </div>
                    <div class="span4">
                      <strong>22</strong>
                      Required
                    </div>
                    <div class="span4">
                      <div class="progress active">
                        <div class="bar progressbar-success" data-percentage="38" style="float: left; width: 38%;"></div>
                        <div class="bar progressbar-danger" data-percentage="10" style="float: right; width: 10%;"></div>
                        <div class="bar progressbar-warning" data-percentage="35" style="float: right; width: 35%;"></div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              {{/is_beta}}
            </a>
          </li>
      {{/related_objects}}
      {{^related_objects}}
      <li class="main no-result">
        No results
      </li>
      {{/related_objects}}
        </ul>
      </section>
    </li>
  </ul>
{{/this}}
