%ul.nav.nav-tabs.nav-stacked
  -@controls.each do |control|
    %li
      %a{ :href => flow_control_path(control), :'data-popover-href' => tooltip_flow_control_path(control) }
        %h3=control.slug
        %p.oneline
          %strong=control.title
          &nbsp;
          =control.description
