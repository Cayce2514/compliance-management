  {{#list}}
  <li class="item" {{data 'model'}} data-model="true" data-id="{{id}}">
    <div class="row-fluid" data-target="#pbc-response-{{id}}" data-toggle="collapse">
      <div class="span4">
        <div class="expander"></div>
        <i class="grcicon-{{system.system_or_process}} process-icon"></i>
        Response using {{system.system_or_process_capitalized}}:
        <span style="color:#333">
          {{system.title}}
        </span>
      </div>
      <div class="span7">
        &nbsp;
      </div>
      <div class="span1 pbc-actions">
        <a class="btn btn-mini delete-response" style="margin-top:0" href="/responses/{{id}}/delete" data-toggle="modal-ajax-form" title="Delete response" rel="tooltip" data-placement="left" data-id="{{id}}">
          <i class="grcicon-x-grey"></i>
        </a>
      </div>    
    </div>
    <div id="pbc-response-{{id}}" class="row-fluid collapse">
      {{#system}}
        {{> /pbc/system.mustache}}
      {{/system}}

      <div class="span4 pbc-response-info">
        <div class="row-fluid">
          <div class="span12">Response Info
          </div>
        </div>
        <div class="row-fluid">
          <div class="span11">
            <label>
              <strong>{{type_name}}</strong>
            </label>
            {{#if_equals type_name 'Documentation'}}
              <ul class="pbc-item-list">
                {{> /pbc/documents_list.mustache}}
                <li class="toggle-add-document add-new disabled">
                  <a href="javascript://">
                    + Add new document
                  </a>
                </li>
              </ul>
            {{/if_equals}}
            {{#if_equals type_name 'Interview'}}
            <label>
              Meeting participants including interviewees
            </label>
            <ul class="pbc-item-list">
              {{#object_people}}
              <li {{data 'model'}} data-model="true" title="{{person.email}}">
                <div class="span5">
                  {{person.name}}
                </div>
                <div class="span6">
                  <select class="edit-person-role">
                    <option value="responsible" {{#if_equals role 'responsible'}}selected="true"{{/if_equals}}>Responsible</option>
                    <option value="accountable" {{#if_equals role 'accountable'}}selected="true"{{/if_equals}}>Accountable</option>
                  </select>
                </div>
                <a class="btn btn-nano pull-right remove_person span1" href="javascript://">
                  <i class="grcicon-x-grey"></i>
                </a>
              </li>
              {{/object_people}}
              <li class="inline-editing inline-add-person hide" data-system-id="{{system.id}}">
                <form>
                  <div class="row-fluid">
                    <div class="span10">
                      <input class="span12 input-title" name="title" placeholder="LDAP or name" type="text" {{ (el) -> el.pbc_autocomplete_people() }}>
                    </div>
                    <div class="span2">
                      <a class="btn btn-mini span12 add-person" data-modal-reset="reset" data-toggle="modal-ajax-form" href="/people/new" data-target="#ajax-modal-people-new">
                        <i class="grcicon-add"></i>
                      </a>
                    </div>
                  </div>
                </form>
              </li>
              <li class="toggle-add-person add-new">
                <a href="javascript://">
                  + Add new participant
                </a>
              </li>
            </ul>
            <label>
              Meetings
            </label>
            <ul class="pbc-item-list">
              {{#meetings}}
              <li {{data 'model'}} data-model="true">
                <div class="span11">
                  <a href="{{calendar_url}}">{{calendar_url}}</a>
                </div>
                <a class="btn btn-nano pull-right remove_person span1" href="javascript://">
                  <i class="grcicon-x-grey"></i>
                </a>
              </li>
              {{/meetings}}
              <li class="inline-editing inline-add-meeting hide" data-system-id="{{system.id}}">
                <form>
                  <div class="row-fluid">
                    <div class="span6">
                      <input class="span12 input-date" name="date" placeholder="choose a meeting date and time" type="text" {{ (el) -> $(el).datepicker(); }}>
                    </div>
                    <div class="span2">
                      <a class="btn btn-mini span12 add-meeting">
                        <i class="grcicon-add"></i>
                      </a>
                    </div>
                  </div>
                </form>
              </li>
              <li class="toggle-add-meeting">
                <a class="span12" data-modal-reset="reset" data-toggle="modal-ajax-form" data-modal-class="modal-wide" href="/meetings/new?meeting[response_id]={{id}}">
                  + Add new meeting
                </a>
              </li>
            </ul>
            {{/if_equals}}
            {{#if_equals type_name 'Population Sample'}}
              {{#population_sample}}
                <label>Population with Sample Requests</label>
                <div data-model="true" {{data 'model'}}>
                  <div class="row-fluid">
                  <ul class="pbc-item-list span10">
                    <li class="pbc-item">
                      <div class="span11">
                        {{#if population_document.link_url}}
                        <a href="{{population_document.link_url}}" target="_blank">{{population_document.title}}{{^population_document.title}}{{population_document.link_url}}{{/population_document.title}}</a>
                        {{else}}
                        <span>{{population_document.title}}</span>
                        {{/if}}
                        &nbsp;
                      </div>
                      <a class="btn btn-nano pull-right toggle-edit-population-doc span1" href="javascript://">
                        <i class="grcicon-edit-grey"></i>
                      </a>
                    </li>
                    <li class="inline-editing inline-edit-population-doc hide" data-doc-type="population">
                      <form>
                        <div class="row-fluid">
                          <div class="span10">
                            <input class="span12 input-title" name="title" placeholder="Doc title or URL" type="text" {{ (el) -> el.pbc_autocomplete_documents() }}>
                          </div>
                          <div class="span2">
                            <a class="btn btn-mini span12 add-document" data-modal-reset="reset" data-toggle="modal-ajax-form" href="/documents/new" data-target="#ajax-modal-documents-new">
                              <i class="grcicon-add"></i>
                            </a>
                          </div>
                        </div>
                      </form>
                    </li>
                  </ul>
                  <div class="span2">
                    <input class="span9 fltrt pull-right" name="" type="text"></input>
                  </div>
                </div>
                  <label>Sample Worksheet</label>
                  <div class="row-fluid">
                    <ul class="pbc-item-list span10">
                      <li class="pbc-item">
                        <div class="span11">
                          {{#if sample_worksheet_document.link_url}}
                          <a href="{{sample_worksheet_document.link_url}}" target="_blank">{{sample_worksheet_document.title}}{{^sample_worksheet_document.title}}{{sample_worksheet_document.link_url}}{{/sample_worksheet_document.title}}</a>
                          {{else}}
                          <span>{{sample_worksheet_document.title}}</span>
                          {{/if}}
                          &nbsp;
                        </div>
                        <a class="btn btn-nano pull-right toggle-edit-population-doc span1" href="javascript://">
                          <i class="grcicon-edit-grey"></i>
                        </a>
                      </li>
                      <li class="inline-editing inline-edit-population-doc hide" data-doc-type="sample_worksheet">
                        <form>
                          <div class="row-fluid">
                            <div class="span10">
                              <input class="span12 input-title" name="title" placeholder="Doc title or URL" type="text" {{ (el) -> el.pbc_autocomplete_documents() }}>
                            </div>
                            <div class="span2">
                              <a class="btn btn-mini span12 add-document" data-modal-reset="reset" data-toggle="modal-ajax-form" href="/documents/new" data-target="#ajax-modal-documents-new">
                                <i class="grcicon-add"></i>
                              </a>
                            </div>
                          </div>
                        </form>
                      </li>
                    </ul>
                    <div class="span2">
                      <input class="span9 fltrt pull-right" name="" type="text"></input>
                    </div>
                  </div>
                  <label>Sample Evidence Collection</label>
                  <ul class="pbc-item-list">
                    <li class="pbc-item">
                      <div class="span11">
                        {{#if sample_evidence_document.link_url}}
                        <a href="{{sample_evidence_document.link_url}}" target="_blank">{{sample_evidence_document.title}}{{^sample_evidence_document.title}}{{sample_evidence_document.link_url}}{{/sample_evidence_document.title}}</a>
                        {{else}}
                        <span>{{sample_evidence_document.title}}</span>
                        {{/if}}
                        &nbsp;
                      </div>
                      <a class="btn btn-nano pull-right toggle-edit-population-doc span1" href="javascript://">
                        <i class="grcicon-edit-grey"></i>
                      </a>
                    </li>
                    <li class="inline-editing inline-edit-population-doc hide" data-doc-type="sample_evidence">
                      <form>
                        <div class="row-fluid">
                          <div class="span10">
                            <input class="span12 input-title" name="title" placeholder="Doc title or URL" type="text" {{ (el) -> el.pbc_autocomplete_documents() }}>
                          </div>
                          <div class="span2">
                            <a class="btn btn-mini span12 add-document" data-modal-reset="reset" data-toggle="modal-ajax-form" href="/documents/new" data-target="#ajax-modal-documents-new">
                              <i class="grcicon-add"></i>
                            </a>
                          </div>
                        </div>
                      </form>
                    </li>
                  </ul>
                </div>
              {{/population_sample}}
            {{/if_equals}}
          </div>
        </div>
      </div>
    </div>
  </li>
  {{/list}}
